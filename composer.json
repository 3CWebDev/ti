{
    "name": "drupal/recommended-project",
    "description": "Project template for Drupal 8 projects with a relocated document root",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "homepage": "https://www.drupal.org/project/drupal",
    "support": {
        "docs": "https://www.drupal.org/docs/user_guide/en/index.html",
        "chat": "https://www.drupal.org/node/314178"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    ],
    "require": {
        "composer/installers": "^1.2",
        "cweagans/composer-patches": "^1.6",
        "drupal/addanother": "^1.0@RC",
        "drupal/address": "^1.8",
        "drupal/admin_toolbar": "^3.0",
        "drupal/antibot": "^2",
        "drupal/back_to_top": "^3",
        "drupal/backup_migrate": "^5.0",
        "drupal/better_exposed_filters": "^6.0",
        "drupal/bootstrap": "^3.21",
        "drupal/captcha": "^2.0",
        "drupal/ckeditor": "^1.0",
        "drupal/coffee": "^1.0",
        "drupal/colorbox": "^2",
        "drupal/content_access": "^2",
        "drupal/core-composer-scaffold": "^10",
        "drupal/core-project-message": "^10",
        "drupal/core-recommended": "^10",
        "drupal/ctools": "^4",
        "drupal/custom_breadcrumbs": "^1.0",
        "drupal/devel": "^5.1",
        "drupal/draggableviews": "^2.0",
        "drupal/eu_cookie_compliance": "^1.9",
        "drupal/field_group": "^3.0",
        "drupal/fiu": "^3.0@beta",
        "drupal/flood_control": "^2.1",
        "drupal/geofield": "^1.13",
        "drupal/geofield_map": "^2.62",
        "drupal/geophp": "^1.0@beta",
        "drupal/google_analytics": "^4.0",
        "drupal/gtm": "^1.6",
        "drupal/honeypot": "^2.0",
        "drupal/image_effects": "^3.0",
        "drupal/jquery_ui": "^1",
        "drupal/jquery_ui_accordion": "^2",
        "drupal/jquery_ui_datepicker": "^2",
        "drupal/jquery_ui_effects": "^2.0",
        "drupal/jquery_ui_menu": "^2.0",
        "drupal/jquery_ui_slider": "^2.0",
        "drupal/jquery_ui_tabs": "^2",
        "drupal/jquery_ui_tooltip": "^2",
        "drupal/login_security": "^2.0",
        "drupal/mailsystem": "^4.3",
        "drupal/menu_link_config": "^1.0@alpha",
        "drupal/metatag": "^2",
        "drupal/module_filter": "^4",
        "drupal/module_missing_message_fixer": "^2.0",
        "drupal/modules_weight": "^3",
        "drupal/nocurrent_pass": "^1.1",
        "drupal/override_node_options": "^2.6",
        "drupal/pathauto": "^1.8",
        "drupal/plupload": "^2.0@beta",
        "drupal/plupload_widget": "^2.0@beta",
        "drupal/prlp": "^1.4",
        "drupal/rabbit_hole": "^1.0@beta",
        "drupal/recaptcha": "^3.0",
        "drupal/recaptcha_v3": "^2.0",
        "drupal/recreate_block_content": "^3",
        "drupal/redirect": "^1.5",
        "drupal/redirect_after_login": "^3",
        "drupal/remove_username": "^1.0",
        "drupal/reroute_email": "^2.0",
        "drupal/responsive_menu": "^4.0",
        "drupal/responsive_tables_filter": "^1.5",
        "drupal/rip": "1.1.x-dev@dev",
        "drupal/security_review": "^2",
        "drupal/sendgrid_integration": "^2",
        "drupal/seo_checklist": "^5.0",
        "drupal/seven": "^1.0",
        "drupal/shield": "^1.4",
        "drupal/simple_sitemap": "^4.0",
        "drupal/site_alert": "^1.0",
        "drupal/slick_views": "^2.2",
        "drupal/stage_file_proxy": "^2.1",
        "drupal/super_login": "^2",
        "drupal/token": "^1.7",
        "drupal/upgrade_status": "^4.0",
        "drupal/url_redirect": "^3",
        "drupal/userprotect": "^1.1",
        "drupal/video_embed_field": "^2.4",
        "drupal/views_accordion": "^2.0",
        "drupal/views_bootstrap": "^5",
        "drupal/webform": "^6.0",
        "drupal/xmlsitemap": "^1.0@RC",
        "drupal/yoast_seo": "^1.5",
        "drush/drush": "^12",
        "geocoder-php/google-maps-provider": "^4.4",
        "php-http/guzzle7-adapter": "^1.0"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "composer/installers": true,
            "cweagans/composer-patches": true,
            "drupal/core-composer-scaffold": true,
            "drupal/core-project-message": true,
            "php-http/discovery": true
        }
    },
    "extra": {
        "drupal-scaffold": {
            "locations": {
                "web-root": "web/"
            },
						"file-mapping": {
							"[web-root]/robots.txt": false,
							"[web-root]/.htaccess": false
					}
        },
				
				"patches": {            										
						
						
				},
        "installer-paths": {
            "web/core": [
                "type:drupal-core"
            ],
            "web/libraries/{$name}": [
                "type:drupal-library"
            ],
            "web/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "web/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "web/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/Commands/contrib/{$name}": [
                "type:drupal-drush"
            ],
            "web/modules/custom/{$name}": [
                "type:drupal-custom-module"
            ],
            "web/themes/custom/{$name}": [
                "type:drupal-custom-theme"
            ]
        },
        "drupal-core-project-message": {
            "include-keys": [
                "homepage",
                "support"
            ],
            "post-create-project-cmd-message": [
                "<bg=blue;fg=white>                                                         </>",
                "<bg=blue;fg=white>  Congratulations, youâ€™ve installed the Drupal codebase  </>",
                "<bg=blue;fg=white>  from the drupal/recommended-project template!          </>",
                "<bg=blue;fg=white>                                                         </>",
                "",
                "<bg=yellow;fg=black>Next steps</>:",
                "  * Install the site: https://www.drupal.org/docs/8/install",
                "  * Read the user guide: https://www.drupal.org/docs/user_guide/en/index.html",
                "  * Get support: https://www.drupal.org/support",
                "  * Get involved with the Drupal community:",
                "      https://www.drupal.org/getting-involved",
                "  * Remove the plugin that prints this message:",
                "      composer remove drupal/core-project-message"
            ]
        }
    },
    "require-dev": {
        "palantirnet/drupal-rector": "^0.15.1"
    }
}
